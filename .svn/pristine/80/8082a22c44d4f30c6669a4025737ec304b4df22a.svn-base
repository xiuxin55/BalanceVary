<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="TextBlock" x:Key="textBlockStyle">
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
    </Style>

    <!--服务月报使用wtt-->
    <Style TargetType="TextBlock" x:Key="textBlockTableTitle">
        <Setter Property="FontSize" Value="14"/>
        <!--<Setter Property="FontFamily" Value="YouYuan"/>-->
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontWeight" Value="Black"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="Background" Value="#eee"/>
        <Setter Property="Padding" Value="32,10,0,0"/>
    </Style>

    <!--服务月报使用wtt-->
    <Style TargetType="TextBlock" x:Key="textBlockTableTitleT">
        <Setter Property="FontSize" Value="14"/>
        <!--<Setter Property="FontFamily" Value="YouYuan"/>-->
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontWeight" Value="Black"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="Background" Value="#eee"/>
        <Setter Property="Padding" Value="70,28,0,0"/>
    </Style>

    <Style TargetType="TextBlock" x:Key="textBlockTableTitleT1">
        <Setter Property="FontSize" Value="14"/>
        <!--<Setter Property="FontFamily" Value="Arial"/>-->
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontWeight" Value="Black"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="Background" Value="#eee"/>
        <Setter Property="Padding" Value="152,10,0,0"/>
    </Style>

    <!--服务月报使用wtt-->
    <Style TargetType="TextBlock" x:Key="textBlockTableTitleY">
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontWeight" Value="Black"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="Background" Value="#eee"/>
        <Setter Property="Padding" Value="16,10,0,0"/>
    </Style>
    <!--服务月报使用wtt-->
    <Style TargetType="TextBlock" x:Key="textBlockTableTitleY1">
        <Setter Property="FontSize" Value="14"/>
        <!--<Setter Property="FontFamily" Value="YouYuan"/>-->
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontWeight" Value="Black"/>
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="Background" Value="#eee"/>
        <Setter Property="Padding" Value="33,10,0,0"/>
    </Style>


    <!--服务月报使用wtt-->
    <Style TargetType="TextBlock" x:Key="textBlockTableContent">
        <Setter Property="FontSize" Value="14"/>
        <!--<Setter Property="FontFamily" Value="Arial"/>-->
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
    </Style>


    <Style TargetType="TextBlock" x:Key="textBlockTitleStyle">
        <Setter Property="FontFamily" Value="宋体"/>
        <!--SimSun-->
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="Foreground" Value="Blue"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>

    <Style TargetType="TextBlock" x:Key="textBlockTitleStyle_Customer">
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="FontWeight" Value="Black"/>
    </Style>
    <!--标题颜色11-10-->
    <Style TargetType="TextBlock" x:Key="textTitleColor">
        <Setter Property="Foreground" Value="Black"/>
    </Style>
    <!--服务预约样式-->
    <Style TargetType="TextBlock" x:Key="textBlockTitleStyle3">
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontSize" Value="24"/>
        <Setter Property="Foreground" Value="#0172d5"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>
    <!--聊天文档标题样式-->
    <Style TargetType="TextBlock" x:Key="textBlockTitleStyle2">
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontSize" Value="24"/>
        <Setter Property="Foreground" Value="#0172d5"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>

    <Style TargetType="TextBlock" x:Key="textBlockStyle2">
        <Setter Property="FontFamily" Value="宋体"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Foreground" Value="Black"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <Style TargetType="TextBox" x:Key="textBoxStyle">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Width" Value="200"/>
        <Setter Property="Height" Value="27"/>
        <Setter Property="Margin" Value="10,0,0,0"/>
    </Style>

    <Style TargetType="ComboBox" x:Key="comboBoxStyle">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Width" Value="200"/>
        <Setter Property="Height" Value="27"/>
        <Setter Property="Margin" Value="10,0,0,0"/>
    </Style>

    <Style TargetType="PasswordBox" x:Key="passwordBoxStyle">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Width" Value="200"/>
        <Setter Property="Height" Value="27"/>
        <Setter Property="Margin" Value="10,0,0,0"/>
    </Style>

    <Style TargetType="DatePicker" x:Key="datePickerStyle">
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Width" Value="200"/>
        <Setter Property="Height" Value="27"/>
        <Setter Property="Margin" Value="10,0,0,0"/>
    </Style>

    <!--背景笔刷1-蓝色水晶-->
    <LinearGradientBrush x:Key="BackGroundBrush1" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFAFBEF0" Offset="0" />
        <GradientStop Color="#FF2140AB" Offset="1" />
        <GradientStop Color="#FF567AF0" Offset="0.049" />
        <GradientStop Color="#FF2140AB" Offset="0.098" />
    </LinearGradientBrush>

    <!--背景笔刷2-蓝紫-->
    <LinearGradientBrush x:Key="BackGroundBrush2" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF2949BE" Offset="0" />
        <GradientStop Color="#FF4F6DCE" Offset="1" />
        <GradientStop Color="#FFBCC9F9" Offset="0.049" />
        <GradientStop Color="#FF2548C1" Offset="0.107" />
    </LinearGradientBrush>

    <!--背景笔刷3-蓝色质感-->
    <LinearGradientBrush x:Key="BackGroundBrush3" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF4771E7" Offset="1" />
        <GradientStop Color="#FFA0BDFF" Offset="0" />
        <GradientStop Color="#FF4771E7" Offset="0.189" />
        <GradientStop Color="#FF4191FF" Offset="0.098" />
    </LinearGradientBrush>

    <!--背景笔刷4-->
    <LinearGradientBrush x:Key="BackGroundBrush4" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF4696E4" Offset="1" />
        <GradientStop Color="#FF366AF7" Offset="0" />
        <GradientStop Color="#FF4696E4" Offset="0.172" />
        <GradientStop Color="#FF2893E0" Offset="0.066" />
    </LinearGradientBrush>

    <!--背景笔刷5-->
    <LinearGradientBrush x:Key="BackGroundBrush5" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF2FA8D6" Offset="1" />
        <GradientStop Color="#FF4AB1E7" Offset="0" />
        <GradientStop Color="#FF3F89EA" Offset="0.115" />
        <GradientStop Color="#FF3992EA" Offset="0.238" />
    </LinearGradientBrush>

    <!--背景笔刷6-->
    <LinearGradientBrush x:Key="BackGroundBrush6" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF2C7AD6" Offset="1" />
        <GradientStop Color="#FF3FCDE0" Offset="0" />
        <GradientStop Color="#FF2C7AD6" Offset="0.107" />
        <GradientStop Color="#FF256CF3" Offset="0.213" />
    </LinearGradientBrush>

    <!--背景笔刷7-->
    <LinearGradientBrush x:Key="BackGroundBrush7" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF2C7AD6" Offset="1" />
        <GradientStop Color="#FF1F85DB" Offset="0" />
        <GradientStop Color="#FF9BD4F7" Offset="0.131" />
        <GradientStop Color="#FFFCFDFE" Offset="0.213" />
    </LinearGradientBrush>
    <!--背景笔刷8-->
    <LinearGradientBrush x:Key="BackGroundBrush8" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="White" Offset="1" />
    </LinearGradientBrush>

    <!--主显区笔刷-MainWindow -->
    <!--<LinearGradientBrush x:Key="MyBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="White"/>
        <GradientStop Color="White" Offset="1"/>
    </LinearGradientBrush>-->
    <LinearGradientBrush x:Key="MyBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="White"/>
        <GradientStop Color="White"  Offset="1"/>
    </LinearGradientBrush>

    <!--标题框笔刷-->

    <LinearGradientBrush x:Key="TitleBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFDEFBFF" Offset="0.016" />
        <GradientStop Color="#FF596CF0" Offset="1" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="TitleBrush2" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFD8F7FF" Offset="0.016" />
        <GradientStop Color="#FF6AAFC4" Offset="1" />
    </LinearGradientBrush>

    <!--弹窗标题栏-->
    <LinearGradientBrush x:Key="WindowTitle" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF2C7AD6" Offset="1" />
        <GradientStop Color="#FF3FCDE0" Offset="0" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="btuCommon" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF4EACF2" Offset="0" />
        <GradientStop Color="#FF1373BE" Offset="1" />
    </LinearGradientBrush>

    <!--ListView线的颜色-->
    <SolidColorBrush Color="Blue" x:Key="col"/>
    <SolidColorBrush Color="#778899" x:Key="colA"/>

</ResourceDictionary>